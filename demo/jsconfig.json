{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@img/*": [
        "src/assets/img/*"
      ],
      "@assets/*": [
        "src/assets/js/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@router/*": [
        "src/router/*"
      ],
      "@com/*": [
        "src/components/*"
      ],
      "@extend/*": [
        "src/extend/*"
      ],
      "@view/*": [
        "src/views/*"
      ],
      "@base/*": [
        "src/views/base/view/*"
      ],
      "@dzph/*": [
        "src/views/dzph/view/*"
      ],
      "@demo/*": [
        "src/views/demo/view/*"
      ],
      "@yyhsglxt/*": [
        "src/views/yyhsglxt/view/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "exclude": [
      "node_modeules",
      "dist"
    ]
  }
}
